<?xml version="1.0" encoding="UTF-8"?>
<!-- mybatis맵퍼  -->
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <!-- namespace속성은 인터페이스의 위치를 적는다. -->
<mapper namespace="com.cafe24.radev.mapper.CheckMapper">
	<select id="getCustomerList" resultType="com.cafe24.radev.vo.Customer">
	  	SELECT 
			customer_code
			,bs_code
			,ei_code
			,customer_sign
			,customer_name
			,customer_birth
			,customer_gender
			,customer_addr
			,customer_phone
			,customer_memo
		FROM customer_tb
	</select>
</mapper>